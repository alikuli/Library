@model ModelsClassLibrary.ModelsNS.ProductNS.Dimensions


<table>
    <thead>
        <tr>
            <th>
                Height
            </th>
            <th>
                Length
            </th>
            <th>
                Width
            </th>

        </tr>
    </thead>

    <tbody>
        <tr>
            <td>
                @Html.EditorFor(model => model.Height, new { htmlAttributes = new { @class = "form-control", disabled = true } })
            </td>
            <td>
                @Html.EditorFor(model => model.Length, new { htmlAttributes = new { @class = "form-control", disabled = true } })
            </td>
            <td>
                @Html.EditorFor(model => model.Width, new { htmlAttributes = new { @class = "form-control", disabled = true } })
            </td>
        </tr>
    </tbody>
</table>
