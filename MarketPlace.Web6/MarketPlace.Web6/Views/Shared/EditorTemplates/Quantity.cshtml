@model ModelsClassLibrary.ModelsNS.ProductNS.Quantity

<div class="" >

    <div class="well p-3 m-3">

        <table class="table" >
            <thead>
                <tr>
                    <th>
                        On Hand
                    </th>
                    <th>
                        On Order
                    </th>
                    <th>
                        Allotted
                    </th>
                    <th>
                        Projected
                    </th>
                </tr>
            </thead>

            <tbody>
                <tr>
                    <td>@Html.EditorFor(model => model.OnHand, new { htmlAttributes = new { @class = "form-control", disabled = true } })</td>
                    <td>@Html.EditorFor(model => model.OnOrder, new { htmlAttributes = new { @class = "form-control", disabled = true } })</td>
                    <td>@Html.EditorFor(model => model.Allotted, new { htmlAttributes = new { @class = "form-control", disabled = true } })</td>
                    <td>@Html.EditorFor(model => model.Projected, new { htmlAttributes = new { @class = "form-control", disabled = true } })</td>
                </tr>
            </tbody>
        </table>













        <table class="table">
            <thead>
                <tr>
                    <th>
                        ERRORS
                    </th>
                    <th>
                        Date
                    </th>
                    <th>
                        Error
                    </th>
                    <th>
                        UOM
                    </th>
                </tr>
            </thead>

            <tbody>
                <tr>
                    <td>
                        <strong>Current</strong>
                    </td>
                    <td>
                        @Html.EditorFor(model => model.LastAllottedDate, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>

                    <td>
                        @Html.EditorFor(model => model.LastQtyError, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                    <td>
                        @Html.EditorFor(model => model.LastUomStockName, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                </tr>
                
                <tr>
                    <td>
                        
                        <strong>Previous</strong>
                    </td>
                    <td>
                        @Html.EditorFor(model => model.PreviousToLastQtyErrorDate, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                    <td>
                        @Html.EditorFor(model => model.PreviousToLastQtyError, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                    <td>
                        @Html.EditorFor(model => model.PreviousToLastUomStockName, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                </tr>


                <tr>
                    <td>
                        
                        <strong>B4 Previous</strong>
                    </td>
                    <td>
                        @Html.EditorFor(model => model.B4PreviousToLastQtyErrorDate, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                    <td>
                        @Html.EditorFor(model => model.B4PreviousToLastQtyError, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                    <td>
                        @Html.EditorFor(model => model.B4PreviousToLastUomStockName, new { htmlAttributes = new { @class = "form-control", disabled = true } })
                    </td>
                </tr>

            </tbody>
        </table>


    </div>



</div>


