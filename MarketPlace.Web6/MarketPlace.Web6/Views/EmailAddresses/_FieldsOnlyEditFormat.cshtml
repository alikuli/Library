@model ModelsClassLibrary.ModelsNS.PlacesNS.EmailAddressNS.EmailAddress

<div class="well p-3 m-3">

    @Html.Partial(@"_Name_And_DetailInfo")

    <div class="form-group">
        @Html.LabelFor(model => model.VerificationNumber, htmlAttributes: new { @class = "" })
        @Html.EditorFor(model => model.VerificationNumber, new { htmlAttributes = new { @class = "form-control", placeholder = "Verification Number" } })
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.VerificationStatusEnum, htmlAttributes: new { @class = "" })
        @Html.DropDownListFor(x => x.VerificationStatusEnum,
                                   (@Model.SelectListAddressVerificationEnum as IEnumerable<SelectListItem>),
                           "--- Default Verification Status ---",
                  new { @class = "form-control" })
    </div>


</div>
